{
  "name": "ticket-sales",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:pro && next build",
    "start": "next start",
    "lint": "next lint",
    "db:reset": "docker compose down && docker compose up -d",
    "prisma:dev": "npx prisma db push && npx prisma generate",
    "prisma:pro": "npx prisma migrate deploy && npx prisma generate",
    "prisma:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seeds/seeds.ts"
  },
  "dependencies": {
